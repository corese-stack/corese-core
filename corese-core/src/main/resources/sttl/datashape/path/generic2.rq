prefix sh: <http://www.w3.org/ns/shacl#> 

template (?shape, ?sh, ?vis, ?ls, ?p, ?o) {
  if (bound(?o), 
    st:call-template(?oper, ?shape, ?sh, ?vis, ?s, ?p, ?b, ?o),
    st:call-template(?oper, ?shape, ?sh, ?vis, ?s, ?p, ?b))
}
where {
  values (?o ?shape ?sh) { (UNDEF UNDEF UNDEF) }
  
  values (?oper ?b) { unnest(sh:getConstraint(sh:generic2, ?shape, ?sh)) }  

  values ?s { unnest(?ls) }

}
