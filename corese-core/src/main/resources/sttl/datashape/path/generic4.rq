prefix sh: <http://www.w3.org/ns/shacl#> 

template  (?shape, ?sh, ?vis, ?ls, ?p, ?o) {
  if (bound(?o), 
    funcall(?oper, ?shape, ?sh, ?vis, ?s, ?p, ?b, ?o),
    funcall(?oper, ?shape, ?sh, ?vis, ?s, ?p, ?b))
}
where {
  graph ?shape {  
    values ?oper { 
      sh:equals 
    }
    ?sh ?oper ?b  
  }

  values ?s { unnest(?ls) }

}
